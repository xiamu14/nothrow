{
  "name": "@redchili/nothrow",
  "version": "0.1.1",
  "description": "Type-safe error handling library with Result pattern",
  "type": "module",
  "private": false,
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "bun build src/index.ts --outdir dist --target node --format esm && bun run build:types",
    "build:types": "tsc --emitDeclarationOnly --declaration --outDir dist",
    "prepublishOnly": "bun run build"
  },
  "keywords": [
    "error-handling",
    "result",
    "typescript",
    "type-safe"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/bun": "latest",
    "typescript": "^5"
  }
}
